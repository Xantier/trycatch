{
  "name": "trycatch",
  "version": "1.0.0",
  "description": "A Testbench wrapper app to automate your testing efforts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xantier/trycatch.git"
  },
  "keywords": [
    "test",
    "automation",
    "test-automation",
    "continuous-delivery",
    "CI/CD"
  ],
  "author": "Jussi Hallila <Jussi@Hallila.com>",
  "license": "SEE LICENSE IN package.json",
  "bugs": {
    "url": "https://github.com/Xantier/trycatch/issues"
  },
  "homepage": "https://github.com/Xantier/trycatch#readme",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node app/run.js",
    "build": "webpack --config webpack.production.config.js",
    "test": "./node_modules/.bin/mocha --reporter spec --compilers js:babel-core/register -r jsdom-global/register './test/**/*.js' ",
    "test-watch": "./node_modules/.bin/mocha --reporter spec --compilers js:babel-core/register -r jsdom-global/register --watch './test/**/*.js' ",
    "test-cov": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha -- -R spec -r jsdom-global/register './test/**/*.js'",
    "lint": "./node_modules/.bin/eslint -c .eslintrc -o ./reports/eslint.xml -f checkstyle app/**/*",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.12.0",
    "lodash": "^4.15.0",
    "loglevel": "^1.4.1",
    "material-ui": "^0.17.2",
    "pretty-data": "^0.40.0",
    "react": "^15.3.2",
    "react-addons-create-fragment": "^15.3.1",
    "react-dom": "^15.3.2",
    "react-ladda": "^4.0.0",
    "react-redux": "^4.4.5",
    "react-tap-event-plugin": "^2.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.11.0",
    "redux-thunk": "^2.0.1",
    "selectly": "^0.4.0",
    "superagent": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-core": "^6.2.1",
    "babel-eslint": "^6.1.2",
    "babel-istanbul": "^0.5.9",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-react-stateless-component-name": "^1.0.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.13.0",
    "chai": "^3.4.1",
    "core-js": "^2.3.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-flowtype": "^2.11.1",
    "eslint-plugin-react": "^3.8.0",
    "flow-bin": "^0.31.1",
    "istanbul": "^0.4.1",
    "jsdom": "^7.1.0",
    "jsdom-global": "^1.7.0",
    "mocha": "^2.3.4",
    "mocha-jsdom": "^1.1.0",
    "proxyquire": "^1.7.3",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^1.3.1",
    "redux-actions": "^0.11.0",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.1",
    "typings": "^1.3.2",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
